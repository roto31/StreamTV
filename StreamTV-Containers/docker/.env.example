# StreamTV Docker Environment Variables
# Copy this file to .env and update with your values

# YouTube API (optional but recommended)
YOUTUBE_API_KEY=

# Archive.org Authentication (optional)
ARCHIVE_ORG_USERNAME=
ARCHIVE_ORG_PASSWORD=

# Plex Integration (optional)
PLEX_BASE_URL=http://192.168.1.100:32400
PLEX_TOKEN=

# Security (optional)
ACCESS_TOKEN=

# Server Base URL (update with your hostname/IP)
# STREAMTV_SERVER_BASE_URL=http://localhost:8410
